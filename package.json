{
  "name": "bumpbot",
  "version": "0.0.1",
  "description": "A basic Discord bot, by the community at The Coding boi",
  "dependencies": {
    "axios": "^0.21.0",
    "discord.js": "^12.5.0",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@jest/globals": "latest",
    "eslint": "^7.14.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "verify": "npm run test && npm run lintfix",
    "start": "node index.js",
    "babel-node": "babel-node --inspect --presets=@babel/preset-env ",
    "dev": "nodemon --exec npm run babel-node -- index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lintfix",
      "pre-push": "npm run verify"
    }
  },
  "engines": {
    "node": "12.x",
    "npm": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlank/bumpbot.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlank/bumpbot/issues"
  },
  "homepage": "https://github.com/carlank/bumpbot#readme"
}
